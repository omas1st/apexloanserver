{
    "version": 2,
    "builds": [
      {
        "src": "apexloanserver/server.js",
        "use": "@vercel/node",
        "config": { "maxLambdaSize": "50mb" }
      },
      {
        "src": "apexloan/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "apexloanserver/server.js" },
      { "src": "/(.*)", "dest": "apexloan/build/$1" }
    ]
  }
  